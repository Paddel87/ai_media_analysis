# Redis-Konfiguration
REDIS_HOST=redis
REDIS_PORT=6379

# Job-Manager-Konfiguration
BATCH_THRESHOLD_HOURS=4
MIN_JOBS_PER_BATCH=3
AUTO_PROCESS_JOBS=false

# GPU-Provider API-Keys
VAST_API_KEY=your_vast_api_key_here
RUNPOD_API_KEY=your_runpod_api_key_here

# AI-Module Konfiguration
POSE_ESTIMATION_MODEL=pare
OCR_DETECTION_MODEL=craft
CLIP_NSFW_MODEL=clip
FACE_REID_MODEL=arcface
WHISPER_MODEL=large-v3

# LLM-Konfiguration
OPENAI_API_KEY=your_openai_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here
CLAUDE_API_KEY=your_claude_api_key_here

# Vector DB Konfiguration
QDRANT_HOST=qdrant
QDRANT_PORT=6333
QDRANT_COLLECTION=media_embeddings

# UI-Konfiguration
STREAMLIT_SERVER_PORT=8501
STREAMLIT_SERVER_ADDRESS=0.0.0.0

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=%(asctime)s - %(name)s - %(levelname)s - %(message)s 